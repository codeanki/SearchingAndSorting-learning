public class Solution{
    public static int binarysearch(int[] arr, int x)[
       int s = 0;
       int e = arr.length -1;
       while(s <= e){
          int mid = (s + e)/2;
          if(arr[mid] > x){
           e = mid - 1;
          }else if(arr[mid] < x){
           s = mid + 1;
          }else{
            return mid;
          }
       }
       return -1;
    }
}
